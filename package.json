{
  "name": "jadelet",
  "version": "2.0.0-pre.1",
  "description": "Lightweight, elegant HTML templates",
  "license": "MIT",
  "devDependencies": {
    "@danielx/coffeecoverage": "4.0.0-pre.1",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "browserify": "^17.0.0",
    "coffeescript": "^2",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "typescript": "^4.6.2",
    "uglify-js": "^3.14.2"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "hera": "STRd6/hera",
    "md5": "^2.3.0",
    "o_0": "^0.4.0-pre.0"
  },
  "homepage": "https://jadelet.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/STRd6/jadelet.git"
  },
  "mocha": {
    "extension": [
      "coffee"
    ],
    "require": [
      "coffeescript/register",
      "@danielx/coffeecoverage/register-istanbul",
      "hera/register",
      "./test/helper.coffee"
    ],
    "reporter": "spec",
    "recursive": true,
    "spec": [
      "test"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".coffee"
    ],
    "exclude": [
      "source/cli.coffee"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "browser": "bash script/browser",
    "prepublish": "bash script/prepublish",
    "test": "nyc mocha"
  },
  "bin": {
    "jadelet": "bin/jadelet"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "main": "dist/jadelet.js"
}
