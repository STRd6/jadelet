{
  "name": "jadelet",
  "version": "2.0.0-pre.8",
  "description": "Lightweight, elegant HTML templates",
  "license": "MIT",
  "devDependencies": {
    "@danielx/coffeecoverage": "4.0.0-pre.1",
    "@danielx/hera": "^0.7.3-pre.3",
    "@types/jsdom": "^16.2.14",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "coffeescript": "^2",
    "esbuild": "^0.14.29",
    "esbuild-coffeescript": "^2.0.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "typescript": "^4.6.2",
    "uglify-js": "^3.14.2"
  },
  "dependencies": {
    "@danielx/observable": "^0.6.0",
    "commander": "^8.1.0",
    "md5": "^2.3.0"
  },
  "homepage": "https://jadelet.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/STRd6/jadelet.git"
  },
  "mocha": {
    "extension": [
      "coffee"
    ],
    "require": [
      "coffeescript/register",
      "@danielx/coffeecoverage/register-istanbul",
      "@danielx/hera/register",
      "./test/helper.coffee"
    ],
    "reporter": "spec",
    "recursive": true,
    "spec": [
      "test"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".coffee"
    ],
    "exclude": [
      "build/",
      "source/cli.coffee"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "compile": "bash script/compile",
    "test": "nyc mocha"
  },
  "bin": {
    "jadelet": "dist/jadelet"
  },
  "files": [
    "dist/",
    "types/",
    "esbuild-plugin.js",
    "esbuild-plugin.d.ts",
    "register.js"
  ],
  "main": "dist/main.js",
  "types": "types/main.d.ts"
}
